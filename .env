# Database Configuration
POSTGRES_USER=appuser
POSTGRES_PASSWORD=apppassword
POSTGRES_MULTIPLE_DATABASES=appdb,keycloak

# MongoDB Configuration
MONGO_INITDB_ROOT_USERNAME=appuser
MONGO_INITDB_ROOT_PASSWORD=apppassword
MONGO_INITDB_DATABASE=appdb

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_DB=postgres
KEYCLOAK_DB_URL=jdbc:postgresql://postgres:5432/keycloak
KEYCLOAK_DB_USERNAME=appuser
KEYCLOAK_DB_PASSWORD=apppassword

# Google OAuth Configuration (Get from Google Cloud Console)
# https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Application Configuration
SERVER_PORT=8080
KRAKEND_PORT=8000
KEYCLOAK_PORT=8180

# JWT Configuration
JWT_ISSUER_URI=http://localhost:8180/realms/springboot-app
JWT_JWK_SET_URI=http://localhost:8180/realms/springboot-app/protocol/openid-connect/certs
